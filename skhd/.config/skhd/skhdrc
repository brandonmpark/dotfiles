ctrl + alt + shift - left : yabai -m window --warp west || $(yabai -m window --display west && sketchybar --trigger windows_on_spaces && yabai -m display --focus west && yabai -m window --warp last) || yabai -m window --move rel:-10:0
ctrl + alt + shift - down : yabai -m window --warp south || $(yabai -m window --display south && sketchybar --trigger windows_on_spaces && yabai -m display --focus south) || yabai -m window --move rel:0:10
ctrl + alt + shift - up : yabai -m window --warp north || $(yabai -m window --display north && sketchybar --trigger windows_on_spaces && yabai -m display --focus north) || yabai -m window --move rel:0:-10
ctrl + alt + shift - right : yabai -m window --warp east || $(yabai -m window --display east && sketchybar --trigger windows_on_spaces && yabai -m display --focus east && yabai -m window --warp first) || yabai -m window --move rel:10:0 

ctrl - o : yabai -m config window_opacity on
ctrl + shift - o : yabai -m config window_opacity off

ctrl + alt - f : yabai -m window --toggle zoom-fullscreen
ctrl + alt + shift - f : yabai -m window --toggle native-fullscreen
 
ctrl + alt - 1 : yabai -m space --focus 1
ctrl + alt - 2 : yabai -m space --focus 2
ctrl + alt - 3 : yabai -m space --focus 3
ctrl + alt - 4 : yabai -m space --focus 4
ctrl + alt - 5 : yabai -m space --focus 5
ctrl + alt - 6 : yabai -m space --focus 6
ctrl + alt - 7 : yabai -m space --focus 7
ctrl + alt - 8 : yabai -m space --focus 8
ctrl + alt - 9 : yabai -m space --focus 9
ctrl + alt - 0 : yabai -m space --focus 10

ctrl + alt - s : /Users/brandonmpark/.config/yabai/modes/monitor.sh

ctrl + alt + shift - 1 : yabai -m window --space 1 \
                  && yabai -m space --focus 1
ctrl + alt + shift - 2 : yabai -m window --space 2 \
                  && yabai -m space --focus 2
ctrl + alt + shift - 3 : yabai -m window --space 3 \
                  && yabai -m space --focus 3
ctrl + alt + shift - 4 : yabai -m window --space 4 \
                  && yabai -m space --focus 4
ctrl + alt + shift - 5 : yabai -m window --space 5 \
                  && yabai -m space --focus 5
ctrl + alt + shift - 6 : yabai -m window --space 6 \
                  && yabai -m space --focus 6
ctrl + alt + shift - 7 : yabai -m window --space 7 \
                  && yabai -m space --focus 7
ctrl + alt + shift - 8 : yabai -m window --space 8 \
                  && yabai -m space --focus 8
ctrl + alt + shift - 9 : yabai -m window --space 9 \
                  && yabai -m space --focus 9
ctrl + alt + shift - 0 : yabai -m window --space 10 \
                  && yabai -m space --focus 10

ctrl + alt - left    : yabai -m window --focus west  || yabai -m display --focus west
ctrl + alt - down    : yabai -m window --focus south || yabai -m display --focus south
ctrl + alt - up    : yabai -m window --focus north || yabai -m display --focus north
ctrl + alt - right : yabai -m window --focus east  || yabai -m display --focus east

cmd + ctrl + alt - left : yabai -m window --resize left:-10:0 || yabai -m window --resize right:-10:0
cmd + ctrl + alt - down : yabai -m window --resize bottom:0:-10 || yabai -m window --resize top:0:10
cmd + ctrl + alt - up : yabai -m window --resize top:0:-10 || yabai -m window --resize bottom:0:10
cmd + ctrl + alt - right : yabai -m window --resize right:10:0 || yabai -m window --resize left:10:0